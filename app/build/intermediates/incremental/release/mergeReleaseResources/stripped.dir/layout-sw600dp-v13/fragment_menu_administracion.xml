<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragments.MenuAdministracion">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MenuAdministracion">

        <Button
            android:id="@+id/button_users_admin_menu"
            style="@style/Widget.FpShare.Button"
            android:layout_marginTop="284dp"
            android:text="@string/users_admin_menu"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button_tags_admin_menu"
            style="@style/Widget.FpShare.Button"
            android:layout_marginBottom="@dimen/size_184"
            android:text="@string/cicles_menu_admin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_users_admin_menu" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>